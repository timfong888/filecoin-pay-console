{
  "name": "filecoin-pay-console",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ga": "NEXT_PUBLIC_CONSOLE_MODE=ga next dev",
    "dev:prototype": "NEXT_PUBLIC_CONSOLE_MODE=prototype next dev",
    "build": "next build",
    "build:ga": "NEXT_PUBLIC_CONSOLE_MODE=ga next build",
    "build:prototype": "NEXT_PUBLIC_CONSOLE_MODE=prototype next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:ga:local": "CONSOLE_MODE=ga playwright test --project=chromium",
    "test:e2e:prototype:local": "CONSOLE_MODE=prototype playwright test --project=chromium",
    "test:e2e:ga:deployed": "TEST_DEPLOYED=ga playwright test --project=chromium",
    "test:e2e:prototype:deployed": "TEST_DEPLOYED=prototype playwright test --project=chromium",
    "test:e2e:all": "npm run test:e2e:ga:local && npm run test:e2e:prototype:local"
  },
  "dependencies": {
    "@filecoin-foundation/ui-filecoin": "^0.7.1",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethers": "^6.16.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "viem": "^2.44.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "msw": "^2.12.7",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.17"
  }
}
